        {
            "categorie": "Gestion des utilisateurs et privilèges",
            "commandes": [
                {
                    "commande": "CREATE USER '<utilisateur>'@'<hôte>' IDENTIFIED BY '<mot_de_passe>';",
                    "description": "Crée un nouvel utilisateur MySQL",
                    "note": "Commande MySQL exécutée après connexion"
                },
                {
                    "commande": "GRANT ALL PRIVILEGES ON <base>.* TO '<utilisateur>'@'<hôte>';",
                    "description": "Accorde tous les privilèges à un utilisateur sur une base",
                    "note": "Commande MySQL exécutée après connexion"
                },
                {
                    "commande": "REVOKE ALL PRIVILEGES ON <base>.* FROM '<utilisateur>'@'<hôte>';",
                    "description": "Révoque tous les privilèges d'un utilisateur",
                    "note": "Commande MySQL exécutée après connexion"
                },
                {
                    "commande": "DROP USER '<utilisateur>'@'<hôte>';",
