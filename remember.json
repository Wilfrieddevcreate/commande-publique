    "Code git pour la création d'une clé ssh": [
        {
            "Étape": "Vérification d'une clé existante",
            "Commande": "ls -al ~/.ssh"
        },
        {
            "Étape": "Générer une nouvelle clé SSH",
            "Commande": "ssh-keygen -t rsa -b 4096 -C \"VbHw5@example.com\"",
            "Note": "\"VbHw5@example.com\" doit être remplacé par votre adresse email."
        },
        {
            "Étape": "Ajouter la clé SSH à l'agent",
            "Commandes": [
                "eval \"$(ssh-agent -s)\"",
                "ssh-add ~/.ssh/id_rsa"
            ]
        },
        {
            "Étape": "Copier la clé publique",
            "Commande": "cat ~/.ssh/id_rsa.pub",
            "Note": "Cette étape dépend  du systeme d'exploitation que vous utilisez."
        },
        {
            "Étape": "Ajouter la clé à GitHub",
            "Instructions": "Collez la clé publique dans votre compte GitHub via les paramètres SSH and GPG keys."
        },
        {
            "Étape": "Tester la connexion",
            "Commande": "ssh -T git@github.com",
            "Résultat attendu": "Hi <username>! You've successfully authenticated, but GitHub does not provide shell access."
        }
    ],
